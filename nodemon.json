{
  "restartable": "rs",
  "ignore": [
      ".git",
      "views",
      "test",
      ".gitkeep",
      ".gitignore",
      "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
      "js": "node --harmony"
  },
  "watch": [
    "**/*.js"
  ],
  "env": {
      "NODE_ENV": "development"
  },
  "ext": "js json",
  "delay": "1500"
}
